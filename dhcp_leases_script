[
:local date [/system clock get date]
:local time [/system clock get time]
:foreach lease in=[/ip dhcp-server lease find dynamic] do={
	/ip dhcp-server lease make-static $lease
	/ip dhcp-server lease comment comment="$date $time" $lease
    /tool fetch url="http://192.168.122.1:5000/conf" http-method=post http-content-type="application/json" http-data="{\"ip_router\":\"$lease\"}"
}
]